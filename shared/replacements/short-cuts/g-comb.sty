\newcommand{\ePermutation}[2]{
  ^{#1}P_{\,#2}
} 

\newcommand{\fPermutation}[2]{
  \dfrac{#1!}{(#1-#2)\,!}
}

\newcommand{\eCombination}[2]{
  ^{#1}C_{\,#2}
} 

\newcommand{\fCombination}[2]{
  \dfrac{#1!}{#2\,!\cdot(#1-#2)\,!}
}
